/**
 * Implements hook_communication_tools_print_alter()
 */
function gmapslivesearch_communication_tools_print_alter(&$block) {
  if (isset($_SESSION['gmapslivesearch_has_results'])) {
    if ($_GET['q'] == variable_get(GMAPSLIVESEARCH_FIELD_BUY_NOW_PAGE_PATH, '') && $_SESSION['gmapslivesearch_has_results']) {
      $block['#markup'] = '<a href="http://maps.google.com/maps?pw=2&q=" id="gmapslivesearch-printresults" class="print-btn">' . t('Print') . '</a>';
      unset($_SESSION['gmapslivesearch_has_results']);
    }
    else if ($_GET['q'] == variable_get(GMAPSLIVESEARCH_FIELD_BUY_NOW_PAGE_PATH_MOBILE, '') && $_SESSION['gmapslivesearch_has_results']) {
      $block['#markup'] = '<a href="http://maps.google.com/maps?pw=2&q=" id="gmapslivesearch-printresults" class="print-btn">' . t('Print') . '</a>';
      unset($_SESSION['gmapslivesearch_has_results']);
    }
  }
}

function gmapslivesearch_return_device_type() {
  $mobile_type = 'desktop';
  if (module_exists('mobile_tools')) {
    $device_type = mobile_tools_get_device();
    $mobile_type = $device_type['type'];
  }
  return $mobile_type;
}

/**
 * Implements hook_communication_tools_destination_alter()
 */
function gmapslivesearch_communication_tools_destination_alter(&$destination) {
  $mobile_type = gmapslivesearch_return_device_type();
  
  if (isset($_POST['gmapslivesearch_field_address_to_search']) && $mobile_type == 'desktop') {
    $buy_now_url = variable_get('gmapslivesearch_field_buy_now_page_path', '');
    $destination = $buy_now_url . '?address=' . $_POST['gmapslivesearch_field_address_to_search'];
  }
  else if (isset($_POST['gmapslivesearch_field_address_to_search']) && $mobile_type == 'mobile') {
    $buy_now_url = variable_get('gmapslivesearch_field_buy_now_page_path_mobile', '');
    $destination = $buy_now_url . '?address=' . $_POST['gmapslivesearch_field_address_to_search'];
  }
}
