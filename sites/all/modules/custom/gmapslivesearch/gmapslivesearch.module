/**
 * Implementation of hook_init()
 */
function gmapslivesearch_init() {
  gmapslivesearch_load_js();
}

function gmapslivesearch_load_js() {
  $device_type = gmapslivesearch_return_device_type();    

  if ($device_type == 'mobile') {
    drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/mobile/gmapslivesearch.mobile.core.js');
    drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/mobile/gmapslivesearch.mobile.js');
  }
  else {
    drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/gmapslivesearch.core.js');
    drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/gmapslivesearch.js');
    
    $apply_jqTransform = variable_get(GMAPSLIVESEARCH_JQTRANSFORM_OPTION, 0);
    drupal_add_js(array('gmapslivesearh_apply_jqtranform' => $apply_jqTransform), 'setting');
  
    if ($apply_jqTransform == 1) {
      drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/jquery.jqtransform.js');
    }
    
  }
}

