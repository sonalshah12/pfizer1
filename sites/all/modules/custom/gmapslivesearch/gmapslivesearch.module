/**
 * Implementation of hook_block_info() 
 */
function gmapslivesearch_block_info() {
  $blocks['gmapslivesearch_main_block']['info'] = t('Google maps live search - Main Block');
  $blocks['gmapslivesearch_find_store']['info'] = t('Google maps live search - Find store and buy online');
  $blocks['gmapslivesearch_find_store']['cache'] = DRUPAL_NO_CACHE;
  return $blocks;
}

/**
 * Implementation of hook_block_view()
 */
function gmapslivesearch_block_view($delta = '') {
  $block = array();
  switch ($delta) {
    case 'gmapslivesearch_main_block':
      $block['content'] = gmapslivesearch_build_results_box();
      break;
    case 'gmapslivesearch_find_store':
      module_load_include('inc', 'gmapslivesearch', 'gmapslivesearch.buynow');
      $block['content'] = drupal_render(drupal_get_form('gmapslivesearch_find_online_form'));
      break;
  }
  return $block;
}

/**
 * Build results box with maps and search returned resuls.
 */
function gmapslivesearch_build_results_box() {
  gmapslivesearch_add_settings_to_js();
  return theme('gmapslivesearch_main_block');
}

/**
 * Build results box with default value of map
 */
function gmapslivesearch_build_default_map($enabled_default_result, $default_result_latitude, $default_result_longitude, $default_result_zoom) {
  drupal_add_js(array('gmapslivesearch_enabled_default_result' => $enabled_default_result), 'setting');
  drupal_add_js(array('gmapslivesearch_default_result_latitude' => $default_result_latitude), 'setting');
  drupal_add_js(array('gmapslivesearch_default_result_longitude' => $default_result_longitude), 'setting');
  drupal_add_js(array('gmapslivesearch_default_result_zoom' => $default_result_zoom), 'setting');
  
  drupal_add_js(array(GMAPSLIVESEARCH_FIELD_GOOGLEAPIKEY => variable_get(GMAPSLIVESEARCH_FIELD_GOOGLEAPIKEY)), 'setting');
  return theme('gmapslivesearch_main_block');
}

/**
 * Get settings for gmapslivesearch and load into JS.
 */
function gmapslivesearch_add_settings_to_js() {
  module_load_include('inc', 'gmapslivesearch', 'gmapslivesearch.buynow');
  $stores_to_search = _gmapslivesearch_get_stores_to_search();

  $mobile_type = gmapslivesearch_return_device_type();
  
  $gmapslivesearch_js_settings = array(
    GMAPSLIVESEARCH_FIELD_SEARCHSTRING => implode('|', $stores_to_search),
    GMAPSLIVESEARCH_FIELD_GOOGLEAPIKEY => variable_get(GMAPSLIVESEARCH_FIELD_GOOGLEAPIKEY), 
  );
  
  if (module_exists('mobile_tools')) {
    if(variable_get('gmapslivesearch_field_retina_image_height') != '') {
      drupal_add_js(array('gmapslivesearch_map_bullet_height' => variable_get('gmapslivesearch_field_retina_image_height')), 'setting');
    }
    if (variable_get('gmapslivesearch_field_retina_image_width') != ''){
      drupal_add_js(array('gmapslivesearch_map_bullet_width' => variable_get('gmapslivesearch_field_retina_image_width')), 'setting');
    }
  }
  
  drupal_add_js(array('gmapslivesearch_results_scroll_js_drag_height' => variable_get('gmapslivesearch_where_to_buy_page_results_scroll_js_drag_height', 73)), 'setting');
  
  drupal_add_js(array('gmapslivesearch' => $gmapslivesearch_js_settings), 'setting');
  drupal_add_js(drupal_get_path('module', 'gmapslivesearch') . '/scripts/jquery.jscrollpane.min.js');
  drupal_add_js(array('gmapslivesearh_map_bullet_folder' => variable_get('file_public_path', 'sites/default/files') . '/' .
      GMAPSLIVESEARCH_MAP_BULLETS_IMAGE_FOLDER), 'setting');
  $first_image = variable_get(GMAPSLIVESEARCH_MAP_BULLETS_IMAGE_PREFIX .'1', NULL);
  if ($first_image) {
    preg_match('/(\.[a-zA-Z]*)?$/', $first_image, $matches);
    drupal_add_js(array('gmapslivesearh_map_bullet_extension' => $matches[1]), 'setting');
  }
  
  if ($mobile_type=='mobile') {
    drupal_add_js(array('gmapslivesearch_show_share' => variable_get(GMAPSLIVESEARCH_FIELD_BUY_NOW_PAGE_SHOW_SHARE_MOBILE, FALSE)), 'setting');
  }
}


